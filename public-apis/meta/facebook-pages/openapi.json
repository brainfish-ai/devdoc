{
  "openapi": "3.0.3",
  "info": {
    "title": "Facebook Pages API",
    "description": "The Facebook Pages API allows you to create, publish, edit, and manage posts on Facebook Pages through the Graph API. Build applications that can post content, respond to comments, manage Page settings, and access insights for Facebook Pages you manage.",
    "version": "v21.0",
    "contact": {
      "name": "Meta for Developers",
      "url": "https://developers.facebook.com/docs/pages-api"
    },
    "x-logo": {
      "url": "https://www.facebook.com/images/fb_icon_325x325.png"
    }
  },
  "servers": [
    {
      "url": "https://graph.facebook.com/v21.0",
      "description": "Facebook Graph API"
    }
  ],
  "tags": [
    {"name": "Posts", "description": "Create, read, update, and delete Page posts"},
    {"name": "Photos", "description": "Upload and manage photos on Pages"},
    {"name": "Videos", "description": "Upload and manage videos on Pages"},
    {"name": "Comments", "description": "Manage comments on Page posts"},
    {"name": "Page Settings", "description": "Manage Page settings and information"},
    {"name": "Insights", "description": "Access Page analytics and insights"},
    {"name": "Conversations", "description": "Manage Page conversations and messages"}
  ],
  "paths": {
    "/{page-id}/feed": {
      "get": {
        "tags": ["Posts"],
        "summary": "Get Page Feed",
        "description": "Returns an array of Post objects on a Facebook Page. Includes posts created by the Page, posts by other users on the Page, and posts that tag the Page.",
        "operationId": "getPageFeed",
        "parameters": [
          {
            "name": "page-id",
            "in": "path",
            "required": true,
            "description": "The ID of the Page",
            "schema": {"type": "string"}
          },
          {
            "name": "fields",
            "in": "query",
            "description": "Comma-separated list of fields to return",
            "schema": {"type": "string", "example": "id,message,created_time,full_picture,permalink_url"}
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Maximum number of posts to return",
            "schema": {"type": "integer", "default": 25}
          },
          {
            "name": "access_token",
            "in": "query",
            "required": true,
            "description": "Page Access Token",
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {"$ref": "#/components/schemas/Post"}
                    },
                    "paging": {"$ref": "#/components/schemas/Paging"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      },
      "post": {
        "tags": ["Posts"],
        "summary": "Create Page Post",
        "description": "Publishes a new post to the Page's feed. You can create text posts, link posts, or schedule posts for future publication.",
        "operationId": "createPagePost",
        "parameters": [
          {
            "name": "page-id",
            "in": "path",
            "required": true,
            "description": "The ID of the Page",
            "schema": {"type": "string"}
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "description": "The main body of the post"
                  },
                  "link": {
                    "type": "string",
                    "description": "URL to attach to the post"
                  },
                  "published": {
                    "type": "boolean",
                    "description": "Set to false to schedule the post",
                    "default": true
                  },
                  "scheduled_publish_time": {
                    "type": "integer",
                    "description": "UNIX timestamp for scheduled publication (10 min to 30 days in future)"
                  },
                  "targeting": {
                    "type": "object",
                    "description": "Targeting options for the post"
                  },
                  "access_token": {
                    "type": "string",
                    "description": "Page Access Token"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Post created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {"type": "string", "description": "The ID of the created post"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      }
    },
    "/{post-id}": {
      "get": {
        "tags": ["Posts"],
        "summary": "Get Post",
        "description": "Returns a single Post object by ID",
        "operationId": "getPost",
        "parameters": [
          {
            "name": "post-id",
            "in": "path",
            "required": true,
            "description": "The ID of the post (format: page-id_post-id)",
            "schema": {"type": "string"}
          },
          {
            "name": "fields",
            "in": "query",
            "description": "Comma-separated list of fields to return",
            "schema": {"type": "string", "example": "id,message,created_time,shares,reactions.summary(true)"}
          },
          {
            "name": "access_token",
            "in": "query",
            "required": true,
            "description": "Page Access Token",
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {"$ref": "#/components/schemas/Post"}
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      },
      "post": {
        "tags": ["Posts"],
        "summary": "Update Post",
        "description": "Updates an existing post. You can only edit posts created by your app.",
        "operationId": "updatePost",
        "parameters": [
          {
            "name": "post-id",
            "in": "path",
            "required": true,
            "description": "The ID of the post to update",
            "schema": {"type": "string"}
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "description": "Updated message text"
                  },
                  "access_token": {
                    "type": "string",
                    "description": "Page Access Token"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Post updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {"type": "boolean"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      },
      "delete": {
        "tags": ["Posts"],
        "summary": "Delete Post",
        "description": "Deletes a post from the Page. You can only delete posts created by your app.",
        "operationId": "deletePost",
        "parameters": [
          {
            "name": "post-id",
            "in": "path",
            "required": true,
            "description": "The ID of the post to delete",
            "schema": {"type": "string"}
          },
          {
            "name": "access_token",
            "in": "query",
            "required": true,
            "description": "Page Access Token",
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Post deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {"type": "boolean"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      }
    },
    "/{page-id}/photos": {
      "get": {
        "tags": ["Photos"],
        "summary": "Get Page Photos",
        "description": "Returns photos uploaded to the Page",
        "operationId": "getPagePhotos",
        "parameters": [
          {
            "name": "page-id",
            "in": "path",
            "required": true,
            "description": "The ID of the Page",
            "schema": {"type": "string"}
          },
          {
            "name": "type",
            "in": "query",
            "description": "Type of photos to return",
            "schema": {"type": "string", "enum": ["uploaded", "tagged"]}
          },
          {
            "name": "access_token",
            "in": "query",
            "required": true,
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {"$ref": "#/components/schemas/Photo"}
                    },
                    "paging": {"$ref": "#/components/schemas/Paging"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      },
      "post": {
        "tags": ["Photos"],
        "summary": "Upload Photo to Page",
        "description": "Uploads a photo to the Page. You can upload from a URL or file.",
        "operationId": "uploadPagePhoto",
        "parameters": [
          {
            "name": "page-id",
            "in": "path",
            "required": true,
            "description": "The ID of the Page",
            "schema": {"type": "string"}
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "source": {
                    "type": "string",
                    "format": "binary",
                    "description": "Photo file to upload"
                  },
                  "url": {
                    "type": "string",
                    "description": "URL of the photo to upload"
                  },
                  "caption": {
                    "type": "string",
                    "description": "Caption for the photo"
                  },
                  "published": {
                    "type": "boolean",
                    "default": true
                  },
                  "access_token": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Photo uploaded successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {"type": "string"},
                    "post_id": {"type": "string"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      }
    },
    "/{page-id}/videos": {
