        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "source": {
                    "type": "string",
                    "format": "binary",
                    "description": "Video file to upload"
                  },
                  "file_url": {
                    "type": "string",
                    "description": "URL of video to upload"
                  },
                  "title": {
                    "type": "string",
                    "description": "Title of the video"
                  },
                  "description": {
                    "type": "string",
                    "description": "Description of the video"
                  },
                  "published": {
                    "type": "boolean",
                    "default": true
                  },
                  "scheduled_publish_time": {
                    "type": "integer",
                    "description": "UNIX timestamp for scheduled publication"
                  },
                  "access_token": {
                    "type": "string"
                  }
                }
              }
            }
          }
