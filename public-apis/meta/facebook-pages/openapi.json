                    "id": {"type": "string"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      }
    },
    "/{object-id}/comments": {
      "get": {
        "tags": ["Comments"],
        "summary": "Get Comments",
        "description": "Returns comments on a post, photo, or video",
        "operationId": "getComments",
        "parameters": [
          {
            "name": "object-id",
            "in": "path",
            "required": true,
            "description": "ID of the post, photo, or video",
            "schema": {"type": "string"}
          },
          {
            "name": "filter",
            "in": "query",
            "description": "Filter comments",
            "schema": {"type": "string", "enum": ["toplevel", "stream"]}
          },
          {
            "name": "order",
            "in": "query",
            "description": "Order of comments",
            "schema": {"type": "string", "enum": ["chronological", "reverse_chronological"]}
          },
          {
            "name": "access_token",
            "in": "query",
            "required": true,
            "schema": {"type": "string"}
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {"$ref": "#/components/schemas/Comment"}
                    },
                    "paging": {"$ref": "#/components/schemas/Paging"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      },
      "post": {
        "tags": ["Comments"],
        "summary": "Post Comment",
        "description": "Posts a comment on a post, photo, or video as the Page",
        "operationId": "postComment",
        "parameters": [
          {
            "name": "object-id",
            "in": "path",
            "required": true,
            "schema": {"type": "string"}
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "description": "Comment text"
                  },
                  "attachment_url": {
                    "type": "string",
                    "description": "URL of image to include"
                  },
                  "access_token": {
                    "type": "string"
                  }
                },
                "required": ["message"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Comment posted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {"type": "string"}
                  }
                }
              }
            }
          }
        },
        "security": [{"PageAccessToken": []}]
      }
    },
